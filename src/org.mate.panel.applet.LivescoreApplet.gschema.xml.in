<schemalist gettext-domain="@GETTEXT_PACKAGE@">
  <schema id="org.mate.panel.applet.LivescoreApplet">
    <key name="leagues-notify" type="s">
      <default>"0"</default>
      <summary>List of leagues to notify about</summary>
      <description>List of leagues for which notificatins are enabled</description>
    </key>
    <key name="leagues-expanded" type="s">
      <default>"0"</default>
      <summary>List of leagues to show expanded</summary>
      <description>List of leagues for which all matches are show expanded in the GUI</description>
    </key>
    <key name="feed" type="i">
      <default>0</default>
      <summary>ID of the selected feed provider</summary>
      <description>ID of the selected matches feed provider</description>
    </key>
  </schema>
</schemalist>
